<%-include("partials/header")-%>
<h1>Compose</h1>
<form action="/compose" method="post">
	<h3>Title</h3>
	<input type="text" name="myposttitle" autocomplete="off" style="width: 100%;">
	<h3>Post</h3>
	<textarea name="mypostcontent" style="width: 100%;height: 30vh;"></textarea>
	<br>
	<button type="submit" style="color:white; background-color: #1ABC9C;border: 1px solid gray;margin-top: 30px;">PUBLISH</button>
</form>
<%-include("partials/footer")-%>